[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1

[libraries]
linux.debug.x86_64 =     "res://target/debug/libdijkstra_map_gd.so"
linux.release.x86_64 =   "res://target/release/libdijkstra_map_gd.so"
windows.debug.x86_64 =   "res://target/debug/dijkstra_map_gd.dll"
windows.release.x86_64 = "res://target/release/dijkstra_map_gd.dll"
macos.debug =            "res://target/debug/libdijkstra_map_gd.dylib"
macos.release =          "res://target/release/libdijkstra_map_gd.dylib"
macos.debug.arm64 =      "res://target/debug/libdijkstra_map_gd.dylib"
macos.release.arm64 =    "res://target/release/libdijkstra_map_gd.dylib"
